<%- menu_items = node.content.public_menu_items.visible_in_sitemap
    menu_items = Cms::ContentsPreloader.new(menu_items).preload(:public_node_ancestors) -%>
<%- if menu_items.present? -%>
<ul class="level<%= level + 1 %> <%= css_class.join('-') %>">
  <%= render partial: 'gnav/public/sitemap/menu_items/menu_item',
             collection: menu_items, locals: { level: level + 1, css_class: css_class } %>
</ul>
<%- end -%>
