<div id="inquiry_style_tags" title="置き換えテキスト" style="display: none;">
  <table>
    <tr>
      <th>グループ名</th><td>@name@</td>
    </tr>
    <tr>
      <th>グループ名（上位グループを含む）</th><td>@full_name@</td>
    </tr>
    <tr>
      <th>住所</th><td>@address@</td>
    </tr>
    <tr>
      <th>TEL</th><td>@tel@</td>
    </tr>
    <tr>
      <th>FAX</th><td>@fax@</td>
    </tr>
    <tr>
      <th>メールアドレス（リンクなし）</th><td>@email@</td>
    </tr>
    <tr>
      <th>メールアドレス（リンクあり）</th><td>@email_link@</td>
    </tr>
    <tr>
      <th>備考</th><td>@note@</td>
    </tr>
  </table>
</div>
