<div class="actionMenu">
<%- if params[:do] == "preview" -%>
<%= action_menu :index %>
<%- else -%>
<%= action_menu :new     if Core.user.has_priv?(:create, :item => Core.concept) %>
<%= action_menu :index %>
<%= action_menu :show %>
<%= action_menu :edit    if safe{@item.editable?} %>
<%= action_menu :destroy if safe{@item.deletable?} %>
<%- end -%>
</div>
