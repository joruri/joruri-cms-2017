<div class="contentGpArticleDocs">
  <div class="searchInfo">
    <%= render 'search_info' %>
  </div>
  <%- unless @docs.empty? -%>
  <div class="docs">
    <%=raw '<ul>' if @content.wrapper_tag == 'li' %>
    <%- @docs.each do |doc| -%>
    <%= content_tag @content.wrapper_tag, doc_replace(doc, @content.list_style, @content.date_style, @content.time_style) %>
    <%- end -%>
    <%=raw '</ul>' if @content.wrapper_tag == 'li' %>
  </div>
  <%- end -%>
</div>
