<div class="contentGpCategoryDoc">
  <%- if @content.feed_display? -%>
  <ul class="feed">
    <li><%= link_to 'RSS' , "index.rss" , class: 'feedRss', title: 'RSSフィードを取得' %></li>
    <li><%= link_to 'Atom', "index.atom", class: 'feedAtom', title: 'Atomフィードを取得' %></li>
  </ul>
  <%- end -%>
  <div class="docs">
    <ul>
      <%- @docs.each do |doc| -%>
      <li class="<%= doc.content.code %>"><%= doc_replace(doc, @content.doc_doc_style, @content.date_style, @content.time_style) %></li>
      <%- end -%>
    </ul>
  </div>

  <%= paginate @docs %>
</div>
