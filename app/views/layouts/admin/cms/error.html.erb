<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title><%= Core.title %></title>
  <link rel="shortcut icon" type="image/vnd.microsoft.icon" href="/_common/favicon.ico" />
  <link rel="icon" type="image/vnd.microsoft.icon" href="/_common/favicon.ico" />
  <%= javascript_include_tag '/_common/js/jquery.min.js' %>
  <%= javascript_include_tag '/_common/js/jquery-migrate.min.js' %>
  <%= javascript_include_tag '/_common/js/jquery_ujs.js' %>

  <%= stylesheet_link_tag '/_common/themes/admin/style.css', media: :all %>
  <%= javascript_include_tag '/_common/js/cms_admin/form.js' %>
  <%= javascript_include_tag '/_common/js/cms_admin/navi.js' %>
  <%= csrf_meta_tags %>
</head>
<body id="errorPage">
  <div id="container">
    <div id="header">
      <div id="headerContainer">
        <%= render 'layouts/admin/cms/header' %>
      <!-- /#headerContainer --></div>
    <!-- /#header --></div>
    <div id="banner">
      <div id="bannerContainer">
      <!-- /#bannerContainer --></div>
    <!-- /#banner --></div>
    <div id="navi">
      <div id="naviContainer">
        <ul class="selector">
          <%= render 'layouts/admin/cms/selector' %>
        </ul>
        <ul class="mainMenu">
          <%= render 'layouts/admin/cms/menu', current: nil %>
        </ul>
      <!-- /#navigationContainer --></div>
    <!-- /#navi --></div>
    <div id="main">
      <div id="content">
        <div id="contentContainer">
          <div id="contentHeader"></div>
          <div id="contentBody">
            <%= yield %>
          </div>
          <div id="contentFooter"></div>
        <!-- /#contentContainer --></div>
      <!-- /#content --></div>
    <!-- /#main --></div>
    <div id="footer">
      <div id="footerContainer">
        <%#= :footer %>
      <!-- /#footerContainer --></div>
    <!-- /#footer --></div>
  <!-- /#container --></div>
</body>
</html>
