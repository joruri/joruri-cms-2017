<%- unless @category_types.empty? -%>
  <%- @category_types.each do |category_type| -%>
    <%- if category_type.public_root_categories.present? -%>
      <ul class="categories">
        <%= render 'categories', categories: category_type.public_root_categories %>
      </ul>
    <%- end -%>
  <%- end -%>
<%- end -%>
