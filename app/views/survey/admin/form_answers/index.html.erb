<%= render 'search' %>

<%= page_entries_info @items %>

<table class="index">
  <tr>
    <th class="action_link">詳細</th>
    <th style="text-align: right; width: 60px;"><%= Survey::FormAnswer.human_attribute_name :id %></th>
    <th style="text-align: center; width: 120px;"><%= Survey::FormAnswer.human_attribute_name :created_at %></th>
    <th style="text-align: left;"><%= Survey::FormAnswer.human_attribute_name :answered_url %></th>
    <th style="text-align: left;"><%= Survey::FormAnswer.human_attribute_name :remote_addr %></th>
    <th style="text-align: left;"><%= Survey::FormAnswer.human_attribute_name :user_agent %></th>
  </tr>
  <%- @items.each do |item| -%>
  <tr class="<%= cycle '', 'cycle' %>">
    <td class="action_link"><%= link_to :show, { action: :show, id: item } %></td>
    <td style="text-align: right;"><%= item.id %></td>
    <td style="text-align: center;"><%=l item.created_at %></td>
    <td style="text-align: left;"><%= link_to item.answered_url_title, item.answered_full_uri, target: 'preview' %></td>
    <td style="text-align: left;"><%= item.remote_addr %></td>
    <td style="text-align: left;"><%= item.user_agent %></td>
  </tr>
  <%- end -%>
</table>

<%= paginate @items %>
