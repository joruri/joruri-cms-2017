<div id="bussinessTimeBox<%= @piece.id %>" class="bussinessTimeBox">
</div>

<script>
$(function() {
  $.ajax({
    url: '<%= @biz_calendar_node_uri %><%= @piece.id %>/bussiness_times<%= "/#{@place_name}" if @place_name.present? %>',
    type: 'GET',
    success: writeBussinnesTime<%= @piece.id %>
  });
  function writeBussinnesTime<%= @piece.id %>(data) {
    $('#bussinessTimeBox<%= @piece.id %>').html(data);
  }
});
</script>
